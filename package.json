{
  "name": "obsidian-smart-image-renamer",
  "version": "0.7.3",
  "description": "Automatically rename pasted images based on the active note's filename",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.0",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "obsidian": "latest",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
